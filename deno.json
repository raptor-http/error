{
  "name": "@raptor/error",
  "version": "0.8.5",
  "exports": "./mod.ts",
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": ["deno.ns", "deno.window"]
  },
  "publish": {
    "exclude": [
      ".github",
      "examples",
      "docs",
      "**/*.test.ts"
    ]
  },
  "tasks": {
    "test": "APP_URL=http://localhost:8000 deno test --coverage=coverage-report --allow-all --parallel",
    "test-coverage": "APP_URL=http://localhost:8000 deno coverage coverage-report"
  },
  "fmt": {
    "exclude": [
      "README.md"
    ]
  },
  "lock": true,
  "imports": {
    "@raptor/framework": "jsr:@raptor/framework@^0.15.0",
    "shiki": "npm:shiki@^3.21.0",
    "ventojs": "npm:ventojs@^2.3.0",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/testing": "jsr:@std/testing@^1.0.16"
  }
}
